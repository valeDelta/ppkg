      <!--INSTALLED UPDATES-->
      <Object Type="If" Key1="'%INSTALLTYPE%'='MSI'">
        <Object Type="Group" Name="Installed Updates" Class="Office Update" GroupingClass="True" Display="Right" TrackProgress="TrackProgress">
